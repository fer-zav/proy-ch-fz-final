  "license": "MIT",
  "name": "clase_16",
  "scripts": {
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "build:css": "node-sass --output-style compressed scss/main.scss styles/main.min.css && npm run css-autoprefixer",
    "css-autoprefixer": "\"postcss -u autoprefixer -r styles/main.min.css\""
  },
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "postcss-cli": "^6.1.3"
  }
}
